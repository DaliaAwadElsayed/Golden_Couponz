<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.goldencouponz.fragments.onboarding.SplashFragment"
        android:label="SplashFragment"
        tools:layout="@layout/splash_fragment">
        <action
            android:id="@+id/firstSkipFragment"
            app:destination="@id/firstSkipFragment" />
    </fragment>
    <fragment
        android:id="@+id/firstSkipFragment"
        android:name="com.goldencouponz.fragments.onboarding.FirstSkipFragment"
        android:label="FirstSkipFragment"
        tools:layout="@layout/first_skip_fragment">
        <action
            android:id="@+id/notificationAlertFragment"
            app:destination="@id/notificationAlertFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationAlertFragment"
        android:name="com.goldencouponz.fragments.onboarding.NotificationAlertFragment"
        android:label="NotificationAlertFragment"
        tools:layout="@layout/notification_alert_fragment" >
        <action
            android:id="@+id/loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/login_fragment"
        android:id="@+id/loginFragment"
        android:name="com.goldencouponz.fragments.authentication.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/registerationFragment"
            app:destination="@id/registerationFragment" />
        <action
            android:id="@+id/forgetPasswordFragment"
            app:destination="@id/forgetPasswordFragment" />
        <action
            android:id="@+id/homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgetPasswordFragment"
        android:name="com.goldencouponz.fragments.authentication.ForgetPasswordFragment"
        android:label="forget_password_fragment"
        tools:layout="@layout/forget_password_fragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.goldencouponz.fragments.home.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/home_fragment" />
    <fragment
        android:id="@+id/registerationFragment"
        android:name="com.goldencouponz.fragments.authentication.RegisterationFragment"
        android:label="registeration_fragment"
        tools:layout="@layout/registeration_fragment" />
</navigation>
